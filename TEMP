str = input()
a=str
b=''
for i,word in enumerate(a):
    if word.isupper==True:
        b+=a[i].lower()
        
    else:
        b+=a[i].upper()


a = input()
b=''
for i,word in enumerate(a):
    if word.isupper():
        a=a.replace(word, word.lower())
    else:
        a=a.replace(word, word.upper())
